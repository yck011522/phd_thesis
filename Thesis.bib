  author = {Deuss, Mario and Panozzo, Daniele and Whiting, Emily and Liu, Yang and Block, Philippe and Sorkine-Hornung, Olga and Pauly, Mark},
  date = {2014-11-19},
  journaltitle = {ACM Transactions on Graphics},
  shortjournal = {ACM Trans. Graph.},
