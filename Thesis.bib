@article{stadelmannEndEffectorPoseCorrection2019,
  title = {End-{{Effector Pose Correction}} for {{Versatile Large-Scale Multi-Robotic Systems}}},
  author = {Stadelmann, Lukas and Sandy, Timothy and Thoma, Andreas and Buchli, Jonas},
