  keywords = {Auto joint,Construction robotics,Construction safety,Rotation method,Steel beam assembly},
  file = {C\:\\Users\\leungp\\Zotero\\storage\\C8WNTPW4\\Liang et al. - 2017 - RAS a robotic assembly system for steel structure.pdf}
}

